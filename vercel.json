{
  "installCommand": "pnpm install --frozen-lockfile",
  "buildCommand": "node scripts/build.ts",
  "outputDirectory": "dist",
  "rewrites": [
    { "source": "/valentines/:path*", "destination": "/valentines/index.html" },
    { "source": "/pairs/:path*", "destination": "/pairs/index.html" },
    { "source": "/world/:path*", "destination": "/world/index.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
